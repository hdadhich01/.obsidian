{
  "settings": {
    "migrated": 4,
    "iconPacksPath": ".obsidian/icons",
    "fontSize": 16,
    "emojiStyle": "none",
    "iconColor": null,
    "recentlyUsedIcons": [],
    "recentlyUsedIconsSize": 5,
    "rules": [
      {
        "rule": "md",
        "icon": "FarFileLines",
        "for": "everything",
        "order": 2,
        "useFilePath": true
      },
      {
        "rule": "pdf",
        "icon": "FarFilePdf",
        "for": "everything",
        "order": 3,
        "useFilePath": true
      },
      {
        "rule": "(png|webpjpeg|jpg|gif|bmp|svg)",
        "icon": "FarFileImage",
